<%-include("header");-%>
<div class="box" id="heading">
     <!-- <h1><%=ListTitle  %>!</h1> -->
     <h1>Today</h1>
</div>

<div class="box">

     <%newListItems.forEach(function(newListItem){%>
     <!-- <form action="/delete" method="post"> -->
          <div class="item">
               <input type="checkbox" />
               <p><%= newListItem.name %></p>
          </div>
     <!-- </form> -->
     <% }) %>

     <form class="item" action="/" method="post">
          <input
               type="text"
               name="ItemName"
               placeholder="new item"
               autocomplete="off"
          />
          <button type="submit" name="list">+</button>
     </form>
</div>

<%-include("footer");-%>
